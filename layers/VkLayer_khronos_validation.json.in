                                                }
                                            ]
                                        },
                                        {
                                            "key": "gpuav_buffer_address_oob",
                                            "label": "Out of bounds buffer device addresses",
                                            "type": "BOOL",
                                            "default": true,
                                            "expanded": true,
                                            "description": "Check for invalid access using buffer device address",
                                            "platforms": [ "WINDOWS", "LINUX" ],
                                            "dependence": {
                                                "mode": "ALL",
                                                "settings": [
                                                    { "key": "gpuav_enable", "value": true },
